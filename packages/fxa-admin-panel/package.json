{
  "name": "fxa-admin-panel",
  "version": "1.205.0",
  "description": "FxA Admin Panel",
  "scripts": {
    "build-postcss": "postcss src/styles/tailwind.css -o src/styles/tailwind.out.css",
    "build-storybook": "NODE_ENV=production npm run build-postcss && build-storybook",
    "build:client": "tsc --build ../fxa-react && tsc --build && NODE_ENV=production npm run build-postcss && SKIP_PREFLIGHT_CHECK=true PUBLIC_URL=/ INLINE_RUNTIME_CHUNK=false CI=false rescripts build",
    "build:server": "tsc -p server/tsconfig.json",
    "build": "npm-run-all build:client build:server",
    "eject": "react-scripts eject",
    "format": "prettier --write --config ../../_dev/.prettierrc '**'",
    "lint": "eslint .",
    "restart": "tsc --build ../fxa-react && npm run build-postcss && pm2 restart pm2.config.js",
    "start": "tsc --build ../fxa-react && npm run build-postcss && pm2 start pm2.config.js",
    "stop": "pm2 stop pm2.config.js",
    "storybook": "npm run build-postcss && start-storybook -p 6009",
    "test:frontend": "SKIP_PREFLIGHT_CHECK=true PUBLIC_URL=/ INLINE_RUNTIME_CHUNK=false CI=true rescripts test --coverage --verbose",
    "test:server": "jest --coverage --verbose --config server/jest.config.js",
    "test": "npm-run-all test:frontend test:server"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@apollo/client": "^3.3.15",
    "body-parser": "^1.19.0",
    "convict": "^6.0.1",
    "convict-format-with-moment": "^6.0.1",
    "convict-format-with-validator": "^6.0.1",
    "dateformat": "^4.3.1",
    "express": "^4.17.1",
    "express-http-proxy": "^1.6.2",
    "fxa-react": "workspace:*",
    "graphql": "^15.5.0",
    "helmet": "^4.5.0",
    "mozlog": "^3.0.1",
    "node-sass": "^4.13.1",
    "npm-run-all": "^4.1.5",
    "on-headers": "^1.0.2",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "react-router-dom": "^5.1.2",
    "react-scripts": "^4.0.3",
    "serve-static": "^1.14.1"
  },
  "devDependencies": {
    "@apollo/react-testing": "3.0.0",
    "@babel/core": "^7.10.3",
    "@rescripts/cli": "0.0.16",
    "@storybook/addon-actions": "^6.2.9",
    "@storybook/addon-links": "^6.2.9",
    "@storybook/addons": "^6.2.9",
    "@storybook/react": "^6.2.9",
    "@tailwindcss/postcss7-compat": "^2.1.2",
    "@testing-library/dom": "^7.24.5",
    "@testing-library/jest-dom": "^5.12.0",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^13.1.5",
    "@types/babel__core": "7.1.14",
    "@types/chance": "^1.1.1",
    "@types/convict": "^5.2.2",
    "@types/dateformat": "^3.0.1",
    "@types/eslint": "7.2.10",
    "@types/express": "^4.17.2",
    "@types/helmet": "4.0.0",
    "@types/jest": "^26.0.22",
    "@types/jsdom": "^12.2.4",
    "@types/on-headers": "^1.0.0",
    "@types/react": "^16.9.53",
    "@types/react-router-dom": "^5.1.7",
    "@types/serve-static": "1.13.9",
    "@types/supertest": "^2.0.11",
    "@types/webpack": "5.28.0",
    "@typescript-eslint/eslint-plugin": "^4.22.1",
    "@typescript-eslint/parser": "^4.22.1",
    "babel-loader": "^8.1.0",
    "chance": "^1.1.4",
    "eslint": "^7.17.0",
    "eslint-plugin-fxa": "^2.0.2",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-react": "^7.22.0",
    "fxa-shared": "workspace:*",
    "jest": "26.6.0",
    "pm2": "^4.5.6",
    "postcss-cli": "^7.1.1",
    "prettier": "^2.0.5",
    "supertest": "^6.1.3",
    "ts-jest": "^26.5.5",
    "ts-node": "^9.1.1",
    "typescript": "3.9.7",
    "webpack": "^4.43.0"
  }
}
