{
  "name": "fxa-shared",
  "version": "1.185.1",
  "description": "Shared module for FxA repositories",
  "main": "dist/index.js",
  "exports": {
    ".": "./dist/index.js",
    "./": "./dist/"
  },
  "scripts": {
    "postinstall": "tsc --build || true",
    "build": "tsc --build",
    "start": "pm2 start pm2.config.js",
    "stop": "pm2 stop pm2.config.js",
    "restart": "pm2 restart pm2.config.js",
    "test": "tsc --build && node ./scripts/mocha-coverage.js -r ts-node/register --recursive test/**/*.{js,ts}",
    "audit": "npm audit --json | audit-filter --nsp-config=.nsprc --audit=-",
    "lint": "eslint .",
    "format": "prettier --write --config ../../_dev/.prettierrc '**'"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mozilla/fxa.git"
  },
  "keywords": [
    "fxa"
  ],
  "author": "Mozilla (https://mozilla.org/)",
  "license": "MPL-2.0",
  "bugs": {
    "url": "https://github.com/mozilla/fxa/issues"
  },
  "engines": {
    "node": ">=12"
  },
  "homepage": "https://github.com/mozilla/fxa/tree/main/packages/fxa-shared#readme",
  "devDependencies": {
    "@types/chai": "^4.2.11",
    "@types/generic-pool": "^3.1.9",
    "@types/lodash.omitby": "^4",
    "@types/lodash.pick": "^4",
    "@types/mocha": "^7.0.2",
    "@types/proxyquire": "^1.3.28",
    "@types/redis": "^2.8.16",
    "@types/sinon": "^9.0.5",
    "audit-filter": "^0.5.0",
    "chai": "^4.2.0",
    "eslint": "^6.8.0",
    "eslint-plugin-fxa": "^2.0.2",
    "jsdom": "16.2.2",
    "jsdom-global": "3.0.2",
    "mocha": "^7.1.2",
    "nyc": "^15.0.1",
    "pm2": "^4.4.1",
    "prettier": "^2.0.5",
    "proxyquire": "^2.1.3",
    "sinon": "^9.0.3",
    "ts-node": "^8.10.2",
    "typescript": "3.9.7",
    "underscore": "^1.10.2",
    "uuid": "^8.3.0"
  },
  "dependencies": {
    "@sentry/browser": "^5.17.0",
    "@types/js-md5": "^0.4.2",
    "accept-language": "^2.0.17",
    "ajv": "^6.12.2",
    "bluebird": "^3.7.2",
    "celebrate": "^10.0.1",
    "cors": "^2.8.5",
    "generic-pool": "^3.7.1",
    "joi": "^14.3.1",
    "js-md5": "^0.7.3",
    "lodash.omitby": "^4.6.0",
    "lodash.pick": "^4.4.0",
    "moment": "^2.24.0",
    "node-uap": "git://github.com/mozilla-fxa/node-uap.git#96dc1f9f224422ec184395b6408cd1fc40ee452a",
    "redis": "^2.8.0",
    "stripe": "^8.69.0"
  }
}
