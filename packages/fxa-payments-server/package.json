    "eslint-plugin-jest": "22.21.0",
    "express": "4.17.1",
    "fxa-content-server": "workspace:*",
    "fxa-geodb": "workspace:*",
    "fxa-react": "workspace:*",
    "fxa-settings": "workspace:*",
    "fxa-shared": "workspace:*",
    "helmet": "3.21.1",
    "hot-shots": "7.4.2",
    "intl-pluralrules": "1.2.0",
    "joi": "14.3.1",
    "morgan": "1.10.0",
    "mozlog": "2.2.0",
