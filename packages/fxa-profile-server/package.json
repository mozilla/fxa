{
  "name": "fxa-profile-server",
  "version": "1.171.1",
  "private": true,
  "description": "Firefox Accounts Profile service.",
  "scripts": {
    "lint": "eslint .",
    "audit": "npm audit --json | audit-filter --nsp-config=.nsprc --audit=-",
    "outdated": "npm outdated --depth 0 || exit 0",
    "preinstall": "./scripts/check_gm.sh",
    "start": "mkdirp var/public && pm2 start pm2.config.js",
    "stop": "pm2 stop pm2.config.js",
    "restart": "pm2 restart pm2.config.js",
    "test": "scripts/test-local.sh",
    "format": "prettier --write --config ../../_dev/.prettierrc '**'"
  },
  "dependencies": {
    "@hapi/boom": "9.1.0",
    "@hapi/catbox-memory": "5.0.0",
    "@hapi/catbox-redis": "5.0.5",
    "@hapi/hapi": "19.1.1",
    "@hapi/inert": "6.0.1",
    "@hapi/joi": "17.1.1",
    "aws-sdk": "2.343.0",
    "bluebird": "3.4.1",
    "buf": "0.1.0",
    "checksum": "0.1.1",
    "compute-cluster": "0.0.9",
    "convict": "6.0.0",
    "convict-format-with-moment": "6.0.0",
    "convict-format-with-validator": "6.0.0",
    "fxa-notifier-aws": "1.0.0",
    "fxa-shared": "workspace:*",
    "gm-reloaded": "1.24.0",
    "lodash": "4.17.15",
    "mozlog": "2.2.0",
    "mysql": "2.16.0",
    "mysql-patcher": "0.7.0",
    "poolee": "1.0.0",
    "raven": "2.1.2",
    "request": "2.83.0",
    "stream-to-array": "2.3.0"
  },
  "devDependencies": {
    "audit-filter": "0.5.0",
    "commander": "2.9.0",
    "eslint": "6.8.0",
    "eslint-plugin-fxa": "2.0.1",
    "insist": "1.0.1",
    "mkdirp": "0.5.1",
    "mocha": "7.2.0",
    "mocha-text-cov": "0.1.1",
    "nock": "8.0.0",
    "nyc": "14.1.1",
    "pm2": "4.4.0",
    "pngparse": "2.0.1",
    "prettier": "2.0.5",
    "rimraf": "2.5.3",
    "sinon": "7.0.0",
    "through": "2.3.8"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mozilla/fxa.git"
  },
  "bugs": "https://github.com/mozilla/fxa/issues",
  "homepage": "https://github.com/mozilla/fxa/tree/master/packages/fxa-profile-server",
  "author": "Sean McArthur <sean.monstar@gmail.com>",
  "engines": {
    "node": ">=12"
  },
  "license": "MPL-2.0"
}
