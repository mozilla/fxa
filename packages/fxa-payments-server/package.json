{
  "name": "fxa-payments-server",
  "version": "1.171.1",
  "description": "Firefox Accounts Payments Service",
  "scripts": {
    "lint": "npm-run-all --parallel lint:eslint lint:sass",
    "lint:eslint": "eslint .",
    "audit": "npm audit --json | audit-filter --nsp-config=.nsprc --audit=-",
    "start": "pm2 start pm2.config.js",
    "stop": "pm2 stop pm2.config.js",
    "restart": "pm2 restart pm2.config.js",
    "build": "tsc --build ../fxa-react && SKIP_PREFLIGHT_CHECK=true PUBLIC_URL=/ INLINE_RUNTIME_CHUNK=false CI=false rescripts build",
    "eject": "react-scripts eject",
    "test": "npm-run-all test:frontend test:server",
    "test:frontend": "SKIP_PREFLIGHT_CHECK=true PUBLIC_URL=/ INLINE_RUNTIME_CHUNK=false CI=yes rescripts test --coverage --verbose",
    "test:server": "jest --coverage --verbose --config server/jest.config.js",
    "format": "prettier --write --config ../../_dev/.prettierrc '**'",
    "storybook": "start-storybook -s ./public -p 6006",
    "build-storybook": "build-storybook && cp -r public/locales public/images storybook-static/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mozilla/fxa.git"
  },
  "keywords": [
    "Firefox Accounts",
    "FxA",
    "Payments"
  ],
  "author": "Mozilla (https://mozilla.org/)",
  "license": "MPL-2.0",
  "bugs": {
    "url": "https://github.com/mozilla/fxa/issues"
  },
  "homepage": "https://github.com/mozilla/fxa/tree/master/packages/fxa-payments-server#README.md",
  "devDependencies": {
    "@babel/core": "7.9.6",
    "@babel/register": "7.9.0",
    "@fluent/bundle": "0.15.1",
    "@fluent/langneg": "0.4.0",
    "@fluent/react": "0.11.1",
    "@rescripts/cli": "0.0.14",
    "@storybook/addon-actions": "5.3.19",
    "@storybook/addon-links": "5.3.19",
    "@storybook/addons": "5.3.19",
    "@storybook/react": "5.3.19",
    "@testing-library/jest-dom": "4.2.4",
    "@testing-library/react": "9.5.0",
    "@types/classnames": "2.2.10",
    "@types/jest": "24.9.1",
    "@types/jsdom": "12.2.4",
    "@types/node": "12.12.42",
    "@types/react": "16.9.35",
    "@types/react-dom": "16.9.8",
    "@types/react-redux": "7.1.9",
    "@types/react-router": "5.1.7",
    "@types/react-router-dom": "4.3.5",
    "@types/redux-actions": "2.6.1",
    "@types/sinon": "7.5.2",
    "@types/storybook__addon-actions": "3.4.3",
    "@types/storybook__addon-links": "3.3.5",
    "@types/storybook__react": "4.0.2",
    "@types/uuid": "7.0.4",
    "@types/webpack": "4.41.13",
    "@typescript-eslint/eslint-plugin": "2.34.0",
    "@typescript-eslint/parser": "2.34.0",
    "async-wait-until": "1.2.4",
    "audit-filter": "0.5.0",
    "babel-eslint": "10.1.0",
    "babel-loader": "8.1.0",
    "browserslist": "4.12.0",
    "caniuse-lite": "1.0.30001066",
    "eslint": "6.8.0",
    "eslint-plugin-fxa": "2.0.1",
    "eslint-plugin-jest": "22.21.0",
    "eslint-plugin-react": "7.20.0",
    "express-http-proxy": "1.6.0",
    "fetch-mock": "8.3.2",
    "handlebars": "4.7.6",
