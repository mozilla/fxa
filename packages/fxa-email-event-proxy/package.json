{
  "name": "fxa-email-event-proxy",
  "version": "1.172.6",
  "description": "Proxies events from Sendgrid to FxA SQS queues",
  "main": "index.js",
  "scripts": {
    "lint": "npm-run-all --parallel lint:*",
    "audit": "npm audit --json | audit-filter --nsp-config=.nsprc --audit=-",
    "lint:eslint": "eslint .",
    "test": "npm run lint && mocha --ui tdd --recursive tests/",
    "build": "zip -q --recurse-paths -y fxa-email-event-proxy *.js *.json node_modules",
    "format": "prettier '**' --write"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mozilla/fxa.git"
  },
  "keywords": [
    "fxa",
    "sendgrid",
    "email",
    "events",
    "proxy"
  ],
  "license": "MPL-2.0",
  "bugs": {
    "url": "https://github.com/mozilla/fxa/issues"
  },
  "homepage": "https://github.com/mozilla/fxa/tree/master/packages/fxa-email-event-proxy#readme",
  "dependencies": {
    "aws-sdk": "^2.653.0",
    "bluebird": "^3.7.2",
    "qs": "^6.9.3"
  },
  "devDependencies": {
    "audit-filter": "^0.5.0",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "eslint": "^6.8.0",
    "eslint-plugin-fxa": "^2.0.1",
    "mocha": "^7.1.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.2",
    "proxyquire": "^2.1.0",
    "sinon": "^9.0.1"
  },
  "engines": {
    "node": ">=12"
  }
}
