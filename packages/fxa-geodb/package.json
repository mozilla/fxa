    "mkdirp": "0.5.1",
    "mozlog": "2.2.0",
    "request": "2.88.0"
  },
  "devDependencies": {
    "audit-filter": "^0.5.0",
    "chai": "^4.2.0",
    "eslint": "^6.8.0",
    "eslint-plugin-fxa": "2.0.1",
    "fxa-shared": "workspace:*",
