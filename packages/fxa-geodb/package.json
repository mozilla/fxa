{
  "name": "fxa-geodb",
  "version": "1.171.0",
  "private": false,
  "description": "Firefox Accounts GeoDB Repo for Geolocation based services",
  "main": "lib/fxa-geodb.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "cover": "nyc _mocha",
    "lint": "npm-run-all --parallel lint:*",
    "audit": "npm audit --json | audit-filter --nsp-config=.nsprc --audit=-",
    "lint:eslint": "eslint .",
    "postinstall": "node scripts/start-db-download.js",
    "test": "mocha",
    "format": "prettier '**' --write"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mozilla/fxa.git"
  },
  "keywords": [
    "geodb",
    "location"
  ],
  "author": "Mozilla (https://mozilla.org/)",
  "license": "MPL-2.0",
  "bugs": {
    "url": "https://github.com/mozilla/fxa/issues"
  },
  "homepage": "https://github.com/mozilla/fxa/tree/master/packages/fxa-geodb",
  "dependencies": {
    "bluebird": "3.5.2",
    "cron": "1.5.0",
    "maxmind": "2.8.0",
    "mkdirp": "0.5.1",
    "mozlog": "2.2.0",
    "request": "2.88.0"
  },
  "devDependencies": {
    "audit-filter": "^0.5.0",
    "chai": "4.2.0",
    "eslint": "6.6.0",
    "eslint-plugin-fxa": "2.0.1",
    "mocha": "5.2.0",
    "npm-run-all": "^4.1.5",
    "nyc": "^14.1.1",
    "prettier": "^1.18.2",
    "rimraf": "2.6.2",
    "sinon": "5.0.7"
  },
  "engines": {
    "node": ">=12"
  }
}
