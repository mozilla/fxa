{
  "name": "fxa-profile-server",
  "version": "1.172.3",
  "private": true,
  "description": "Firefox Accounts Profile service.",
  "scripts": {
    "lint": "npm-run-all --parallel lint:*",
    "lint:eslint": "eslint .",
    "audit": "npm audit --json | audit-filter --nsp-config=.nsprc --audit=-",
    "outdated": "npm outdated --depth 0 || exit 0",
    "preinstall": "./scripts/check_gm.sh",
    "prestart": "mkdirp var/public",
    "start": "pm2 start pm2.config.js",
    "stop": "pm2 stop pm2.config.js",
    "restart": "pm2 restart pm2.config.js",
    "test": "scripts/test-local.sh",
    "format": "prettier '**' --write"
  },
  "dependencies": {
    "@hapi/boom": "~9.1.0",
    "@hapi/catbox-memory": "~5.0.0",
    "@hapi/catbox-redis": "~5.0.5",
    "@hapi/hapi": "19.1.1",
    "@hapi/inert": "~6.0.1",
    "@hapi/joi": "^17.1.1",
    "aws-sdk": "2.343.0",
    "bluebird": "3.4.1",
    "buf": "0.1.0",
    "checksum": "0.1.1",
    "compute-cluster": "0.0.9",
    "convict": "^6.0.0",
    "convict-format-with-moment": "^6.0.0",
    "convict-format-with-validator": "^6.0.0",
    "fxa-notifier-aws": "1.0.0",
    "gm-reloaded": "1.24.0",
    "lodash": "~4.17.15",
    "mozlog": "2.2.0",
    "mysql": "2.16.0",
    "mysql-patcher": "0.7.0",
    "poolee": "1.0.0",
    "raven": "2.1.2",
    "request": "2.83.0",
    "stream-to-array": "2.3.0"
  },
  "devDependencies": {
    "audit-filter": "^0.5.0",
    "commander": "2.9.0",
    "eslint": "6.6.0",
    "eslint-plugin-fxa": "2.0.1",
    "fxa-shared": "file:../fxa-shared",
    "grunt": "1.0.4",
    "grunt-copyright": "0.3.0",
    "grunt-eslint": "^22.0.0",
    "grunt-mocha-test": "0.12.7",
    "insist": "1.0.1",
    "load-grunt-tasks": "3.5.0",
    "mkdirp": "0.5.1",
    "mocha": "4.0.1",
    "mocha-text-cov": "0.1.1",
    "nock": "8.0.0",
    "npm-run-all": "^4.1.5",
    "nyc": "^14.1.1",
    "pm2": "^4.2.3",
    "pngparse": "2.0.1",
    "prettier": "^1.18.2",
    "rimraf": "2.5.3",
    "sinon": "7.0.0",
    "through": "2.3.8",
    "time-grunt": "1.3.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mozilla/fxa.git"
  },
  "bugs": "https://github.com/mozilla/fxa/issues",
  "homepage": "https://github.com/mozilla/fxa/tree/master/packages/fxa-profile-server",
  "author": "Sean McArthur <sean.monstar@gmail.com>",
