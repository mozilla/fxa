{
  "name": "fxa-payments-server",
  "version": "1.149.2",
  "description": "Firefox Accounts Payments Service",
  "scripts": {
    "lint": "npm-run-all --parallel lint:*",
    "lint:eslint": "eslint .",
    "lint:sass": "sass-lint -v",
    "lint:deps": "npm audit --json | audit-filter --nsp-config=.nsprc --audit=-",
    "start": "node server/bin/fxa-payments-server.js",
    "start-dev": "npm-run-all --parallel server:proxy server:react-scripts",
    "server:proxy": "PROXY_STATIC_RESOURCES_FROM='http://127.0.0.1:3032' node server/bin/fxa-payments-server.js",
    "server:react-scripts": "react-scripts start",
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "test": "npm-run-all test:*",
    "test:frontend": "react-scripts test --coverage --verbose",
    "test:server": "jest --coverage --verbose --config server/jest.config.js",
    "format": "prettier '**' --write",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mozilla/fxa.git"
  },
  "keywords": [
    "Firefox Accounts",
    "FxA",
    "Payments"
  ],
  "author": "Mozilla (https://mozilla.org/)",
  "license": "MPL-2.0",
  "bugs": {
    "url": "https://github.com/mozilla/fxa/issues"
  },
  "homepage": "https://github.com/mozilla/fxa/tree/master/packages/fxa-payments-server#README.md",
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/register": "^7.4.4",
    "@storybook/addon-actions": "^5.1.11",
    "@storybook/addon-links": "^5.1.11",
    "@storybook/addons": "^5.1.11",
    "@storybook/react": "^5.1.11",
    "@testing-library/jest-dom": "^4.0.0",
    "@testing-library/react": "^9.1.1",
    "@types/classnames": "^2.2.9",
    "@types/jest": "^24.0.17",
    "@types/jsdom": "^12.2.4",
    "@types/node": "^12.7.1",
    "@types/react": "^16.9.1",
    "@types/react-dom": "^16.8.5",
    "@types/react-redux": "^7.1.1",
    "@types/react-router": "^5.0.3",
    "@types/react-router-dom": "^4.3.3",
    "@types/redux-actions": "^2.6.0",
    "@types/sinon": "^7.0.11",
    "@types/storybook__addon-actions": "^3.4.2",
    "@types/storybook__addon-links": "^3.3.5",
    "@types/storybook__react": "^4.0.1",
    "@typescript-eslint/eslint-plugin": "^1.9.0",
    "@typescript-eslint/parser": "^1.9.0",
    "audit-filter": "^0.5.0",
    "babel-eslint": "^10.0.2",
    "babel-loader": "^8.0.6",
    "browserslist": "^4.6.6",
    "caniuse-lite": "^1.0.30000989",
    "eslint": "^6.2.1",
    "express-http-proxy": "^1.5.1",
    "nock": "^10.0.6",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.18.2",
    "redux-devtools-extension": "^2.13.8",
    "sass-lint": "^1.13.1",
    "supertest": "4.0.2",
    "ts-jest": "^24.0.2",
    "ts-node": "^8.1.0",
    "typescript": "^3.4.5",
    "wait-for-expect": "^1.3.0"
  },
  "dependencies": {
    "@sentry/browser": "^5.6.1",
    "@sentry/node": "^5.7.1",
    "@types/nock": "^10.0.1",
    "@types/react-stripe-elements": "^1.3.1",
    "celebrate": "^10.0.1",
    "classnames": "^2.2.6",
    "convict": "^5.1.0",
    "dayjs": "^1.8.15",
    "eslint-plugin-jest": "^22.15.1",
    "express": "^4.16.4",
    "helmet": "3.21.1",
    "joi": "^14.3.1",
    "morgan": "^1.9.1",
    "mozlog": "^2.2.0",
    "node-sass": "^4.12.0",
    "nyc": "^14.1.0",
    "on-headers": "^1.0.2",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-redux": "^7.1.0-alpha.4",
    "react-router-dom": "^5.0.0",
    "react-scripts": "^3.1.1",
    "react-stripe-elements": "^5.0.1",
    "react-test-renderer": "^16.9.0",
    "redux": "^4.0.4",
    "redux-actions": "^2.6.5",
    "redux-promise-middleware": "^6.1.0",
    "redux-thunk": "^2.3.0",
    "serve-static": "^1.13.2",
    "speed-trap": "0.0.10",
    "type-to-reducer": "^1.1.0"
  },
  "engines": {
    "node": ">=10",
    "npm": ">=6.4.1"
  },
  "readmeFilename": "README.md",
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!**/node_modules/*",
      "!**/test-utils.*",
      "!**/build/*",
      "!**/*.stories.*",
      "!**/types.tsx",
      "!**/*.d.ts",
      "!**/jest*js"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 94,
        "functions": 84,
        "lines": 88,
        "statements": 88
      }
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
