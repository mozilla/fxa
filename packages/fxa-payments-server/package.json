    "nyc": "14.1.1",
    "pm2": "4.4.0",
    "prettier": "2.0.5",
    "redux-devtools-extension": "2.13.8",
    "sinon": "7.5.0",
    "supertest": "4.0.2",
    "ts-jest": "24.3.0",
    "ts-node": "8.10.1",
