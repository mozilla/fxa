{
  "name": "fxa-settings",
  "version": "1.193.0",
  "homepage": "https://accounts.firefox.com/beta/settings",
  "private": true,
  "scripts": {
    "build-postcss": "postcss src/styles/tailwind.scss -o src/styles/tailwind.out.scss",
    "build-storybook": "NODE_ENV=production STORYBOOK_BUILD=1 npm run build-postcss && build-storybook -s .storybook/design-guide",
    "build": "tsc --build ../fxa-react && NODE_ENV=production npm run build-postcss && SKIP_PREFLIGHT_CHECK=true INLINE_RUNTIME_CHUNK=false rescripts build",
    "eject": "react-scripts eject",
    "lint:eslint": "eslint .",
    "lint": "npm-run-all --parallel lint:eslint lint:sass",
    "restart": "npm run build-postcss && pm2 restart pm2.config.js",
    "start": "npm run build-postcss && pm2 start pm2.config.js",
    "stop": "pm2 stop pm2.config.js",
    "storybook": "npm run build-postcss && start-storybook -p 6008 -s .storybook/design-guide --no-version-updates",
    "test": "SKIP_PREFLIGHT_CHECK=true rescripts test"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@apollo/client": "^3.2.2",
    "@reach/router": "^1.3.4",
    "base32-decode": "^1.0.0",
    "base32-encode": "^1.1.1",
    "classnames": "^2.2.6",
    "fxa-auth-client": "workspace:*",
    "fxa-common-password-list": "^0.0.4",
    "fxa-react": "workspace:*",
    "graphql": "^14.6.0",
    "lodash.groupby": "^4.6.0",
    "postcss-import": "^12.0.1",
    "react": "^17.0.1",
    "react-async-hook": "^3.6.1",
    "react-dom": "^16.13.1",
    "react-hook-form": "^6.11.0",
    "react-scripts": "^3.4.1",
    "subscriptions-transport-ws": "^0.9.18",
    "typescript": "3.9.7"
  },
  "devDependencies": {
    "@babel/core": "^7.10.3",
    "@rescripts/cli": "0.0.14",
    "@sentry/browser": "^5.27.1",
    "@storybook/addon-actions": "^5.3.19",
    "@storybook/addon-links": "^6.0.28",
    "@storybook/addons": "^5.3.19",
    "@storybook/preset-create-react-app": "^2.1.1",
    "@storybook/react": "^5.3.19",
    "@testing-library/dom": "^7.24.5",
    "@testing-library/jest-dom": "^5.11.0",
    "@testing-library/react": "^9.5.0",
    "@testing-library/react-hooks": "^3.4.2",
    "@testing-library/user-event": "^12.1.7",
    "@types/babel__core": "7.1.7",
    "@types/classnames": "^2.2.10",
    "@types/jest": "^24.9.1",
    "@types/lodash.groupby": "^4",
    "@types/node": "^14.14.5",
    "@types/react": "^16.9.53",
    "@types/react-dom": "^16.9.6",
    "@types/react-test-renderer": "^16",
    "@types/testing-library__react-hooks": "^3",
    "@types/webpack": "4.41.16",
    "babel-loader": "^8.0.0",
    "eslint": "^6.8.0",
    "eslint-plugin-fxa": "^2.0.2",
    "eslint-plugin-jest": "^23.13.2",
    "eslint-plugin-react": "7.19.0",
    "fxa-shared": "workspace:*",
    "mutationobserver-shim": "^0.3.7",
    "node-sass": "^4.14.0",
    "npm-run-all": "^4.1.5",
    "pm2": "^4.4.1",
    "postcss-cli": "^7.1.1",
    "react-test-renderer": "^17.0.1",
    "tailwindcss": "^1.9.1",
    "webpack": "^4.43.0"
  }
}
