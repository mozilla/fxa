{
  "extends": ["plugin:fxa/client"],
  "plugins": ["fxa"],
  "parser": "babel-eslint",
  "parserOptions": {
    "sourceType": "module"
  },
  "rules": {
    "no-console": "off",
    "camelCase": "off",
    "strict": "off",
    "no-useless-escape": "off",
    "handle-callback-err": "off",
    "react/jsx-uses-vars": "on"
  }
}
