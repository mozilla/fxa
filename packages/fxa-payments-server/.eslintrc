{
  "extends": [
    "plugin:fxa/server",
    "plugin:jest/recommended"
  ],
  "plugins": ["fxa"],
  "parser": "babel-eslint",
  "parserOptions": {
    "sourceType": "module"
  },
  "rules": {
    "camelcase": "off",
    "strict": "off",
    "no-useless-escape": "off",
    "handle-callback-err": "off",
    "react/jsx-uses-vars": "on"
  }
}
