{
  "extends": [
    "../../.eslintrc",
    "plugin:jest/recommended"
  ],
  "parser": "babel-eslint",
  "rules": {
    "no-console": "off",
    "camelcase": "off",
    "strict": "off",
    "no-useless-escape": "off",
    "handle-callback-err": "off"
  },
  "parserOptions": {
    "sourceType": "module"
  }
}
